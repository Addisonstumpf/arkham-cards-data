{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "schema/log.schema.json",
  "definitions": {
    "log": {
      "$id": "#log",
      "title": "Log",
      "type": "object",
      "properties": {
        "campaignName": {
          "type": "string"
        },
        "campaignCode": {
          "type": "string"
        },
        "log": {
          "type": "array",
          "items": {
            "$ref": "logEntry.schema.json#/definitions/logEntry"
          }
        }
      },
      "required": ["campaignName", "campaignCode", "log"],
      "additionalProperties": false
    }
  }
}
